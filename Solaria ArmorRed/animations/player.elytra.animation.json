			}
		},

		"animation.player.elytra.normal": {
			"loop": true,
			"bones": {
				"left_wing": {
					"position": [ 4.65, 4.3, -2.0 ],
					"rotation": [ 15, 0, -13 ],
					"scale": [ 1.075, 1.075, 2.25 ]
				},
				"right_wing": {
