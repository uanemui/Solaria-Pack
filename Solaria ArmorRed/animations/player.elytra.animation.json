				}
			}
		},
		// Solaria & Prokits St.
		"animation.player.elytra.gliding": {
			"loop": true,
			"bones": {
				"left_wing": {
					"position": [ 2, 2, -2 ],
					"rotation": [ "variable.ratio = (query.position_delta(1) < 0) ? 1 - math.pow(-query.movement_direction(1), 1.5) : 1; return math.lerp(15, 20, variable.ratio);", 0, "math.lerp(-15, -90, variable.ratio)" ],
					"scale": [ 1, 1, 2 ]
				},
				"right_wing": {
					"position": [ -2, 2, -2 ],
					"rotation": [ "variable.ratio = (query.position_delta(1) < 0) ? 1 - math.pow(-query.movement_direction(1), 1.5) : 1; return math.lerp(15, 20, variable.ratio);", 0, "math.lerp(15, 90, variable.ratio)" ],
					"scale": [ 1, 1, 2 ]
				}
			}
		},
		"animation.player.elytra.sleeping": {
			"loop": true,
			"bones": {
				"left_wing": {
					"position": [ 10 ,10 ,10 ],
					"rotation": [ 0, 0, -15 ],
					"scale": [ 1, 1, 2 ]
				},
				"right_wing": {
					"position": [ -5, 4, -2 ],
					"rotation": [ 0, 0, 15 ],
					"scale": [ 1, 1, 2 ]
				}
			}
		},
		"animation.player.elytra.swimming": {
			"loop": true,
			"bones": {
				"left_wing": {
					"position": [ 5, 2, -2 ],
					"rotation": [ "query.swim_amount * 15", 0, -15 ],
					"scale": [ 1, 1, 2 ]
				},
				"right_wing": {
					"position": [ -5, 2, -2 ],
					"rotation": [ "query.swim_amount * 15", 0, 15 ],
					"scale": [ 1, 1, 2 ]
				}
			}
		}
	}
}
